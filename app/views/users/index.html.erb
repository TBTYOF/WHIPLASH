<div class="container">
	<h1>Users</h1>
	<% @users.each do |user| %>
	<%= link_to user.name, user_path(user) %>
	<%= link_to user_path(user) do %>
	<%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpg', fallback: "no_image.jpg", size:'60x60' %>
	<% end %>

	<%= user.profile_text %>
	<% end %>

</div>
